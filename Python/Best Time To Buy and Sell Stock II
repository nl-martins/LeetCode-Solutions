class Solution(object):
    def maxProfit(self, prices):
        """
        :type prices: List[int]
        :rtype: int
        """
        max_profit = 0
        for i,x in enumerate(prices[1:]) :
            if x > prices[i] :						#if current price is greater than last one - > take profit
            	max_profit += x - prices [i]		
            
        return max_profit
